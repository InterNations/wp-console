{
  "alias": "wordpress.phar",
  "chmod": "0755",
  "directories": [
    "config",
    "src",
    "templates"
  ],
  "files": [
    "config.yml",
    "services.yml",
    "bin/wp-console.php",
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "name": "/(\\.php|\\.json|\\.yml|\\.twig)$/",
      "exclude": ["Tests","tests", "docs"],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "main": "bin/wp-console",
  "output": "wordpress.phar",
  "stub": true
}
